<Application x:Class="dot_Installer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:dot_Installer"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor" Color="#fff" />
        <SolidColorBrush x:Key="BackgroundBoxColor" Color="#fcfcfc" />
        <SolidColorBrush x:Key="ForegroundColor" Color="#000" />
        <SolidColorBrush x:Key="Border" Color="#B9B9B9"/>
    </Application.Resources>
</Application>
